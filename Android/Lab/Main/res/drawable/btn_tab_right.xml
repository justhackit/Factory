<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_window_focused="false" android:state_checked="true" android:drawable="@drawable/tab_bar_item_selected_right" />
    <item android:state_window_focused="false" android:state_checked="false" android:drawable="@drawable/tab_bar_item_default_right" />
    <item android:state_checked="true" android:state_pressed="true" android:drawable="@drawable/tab_bar_item_pressed_right" />
    <item android:state_checked="false" android:state_pressed="true" android:drawable="@drawable/tab_bar_item_pressed_right" />
    <item android:state_focused="true" android:state_checked="true" android:drawable="@drawable/tab_bar_item_focused_right" />
    <item android:state_focused="true" android:state_checked="false" android:drawable="@drawable/tab_bar_item_focused_right" />
    <item android:state_checked="false" android:drawable="@drawable/tab_bar_item_default_right" />
    <item android:state_checked="true" android:drawable="@drawable/tab_bar_item_selected_right" />
</selector>